{
  "name": "fravegaautomation",
  "version": "1.0.0",
  "description": "Automatizaciones de test en la pagina web de Fravega",
  "license": "ISC",
  "author": "Nicolas Pavon",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:run": "cypress run --browser chrome --spec cypress/e2e/**/*.js --env allure=true",
    "allure:report": "npx allure generate allure-results -o allure-report",
    "allure:open": "npx allure open",
    "test-entero": "npm run cy:run ; npm run allure:report && npm run allure:open",
    "last" : "node run_all.js"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.1",
    "cypress": "^15.7.0"
  }
}
